(fun nfibs (n) 
    (if (= n 0) 
        0 
        (if (< n 2) 
            1 
            (+ (nfibs (- n 1)) (nfibs (- n 2)))
        )
    )
)

(fun main() 
    (print (nfibs 0))
    (print (nfibs 1))
    (print (nfibs 2))
    (print (nfibs 3))
    (print (nfibs 4))
    (print (nfibs 5))
    (print (nfibs 6))
